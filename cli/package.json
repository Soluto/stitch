{
  "name": "stitch-cli",
  "version": "0.0.17",
  "author": "Aviv Rozenboim @AvivRubys",
  "repository": "Soluto/stitch",
  "homepage": "https://github.com/Soluto/stitch",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "stitch": "./bin/run"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "build": "tsc",
    "test": "mocha -r ts-node/register 'tests/**/*.spec.ts'",
    "version": "oclif-dev readme && git add README.md"
  },
  "bugs": "https://github.com/Soluto/stitch/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "globby": "^11.0.1",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "js-yaml": "^3.14.1",
    "ramda": "^0.27.1",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.25.1",
    "@oclif/test": "^1.2.8",
    "@types/js-yaml": "^3.12.5",
    "@types/mocha": "^8.2.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.11",
    "@types/ramda": "^0.27.34",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "nock": "^13.0.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "stitch",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
